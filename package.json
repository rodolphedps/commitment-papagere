{
  "name": "papagere-front",
  "version": "1.0.0",
  "description": "An application that simplifies your life",
  "engines": {
    "npm": ">=3",
    "node": ">=v10.9.0",
    "yarn": ">=1.9.4"
  },
  "author": "Johan le Roch",
  "scripts": {
    "prebuild": "yarn build:clean",
    "build:prod": "yarn prebuild && cross-env NODE_ENV=production webpack --config config/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:staging": "yarn prebuild && cross-env NODE_ENV=staging webpack --config config/webpack.staging.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "yarn build:prod && yarn start:prod",
    "start:staging": "yarn build:staging && cross-env NODE_ENV=staging SENTRY_ACTIVE=false node server",
    "start:prod": "cross-env NODE_ENV=production SENTRY_ACTIVE=false node server",
    "clean": "yarn run test:clean && yarn run build:clean",
    "lint": "yarn run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "eslint:fix": "eslint -- . --ignore-path .gitignore --fix",
    "lint:js": "yarn run lint:eslint . ",
    "pretest": "yarn run test:clean && yarn run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy:staging": "yarn build:staging && yarn aws:s3:upload-staging && yarn aws:s3:upload-staging-cached-files && yarn aws:cloudfront:invalidate-staging",
    "deploy:prod": "yarn build:prod && yarn aws:s3:upload-prod && yarn aws:s3:upload-prod-cached-files && yarn aws:cloudfront:invalidate-prod",
    "aws:s3:upload-staging": "aws s3 sync build/ s3://papagere-staging --acl public-read --profile papagere --exclude \"index.html\" --exclude \"ppg-sw.js\" --exclude \"*manifest.*.js*\"",
    "aws:s3:upload-staging-cached-files": "aws s3 sync build/ s3://papagere-staging --acl public-read --profile papagere --exclude \"*\" --include \"index.html\" --include \"ppg-sw.js\" --include \"*manifest.*.js*\" --cache-control \"no-cache, no-store, must-revalidate\" --expires 0",
    "aws:s3:upload-prod": "aws s3 sync build/ s3://papagere --acl public-read --profile papagere --exclude \"index.html\" --exclude \"ppg-sw.js\" --exclude \"*manifest.*.js*\"",
    "aws:s3:upload-prod-cached-files": "aws s3 sync build/ s3://papagere --acl public-read --profile papagere --exclude \"*\" --include \"index.html\" --include \"ppg-sw.js\" --include \"*manifest.*.js*\" --cache-control \"no-cache, no-store, must-revalidate\" --expires 0",
    "aws:cloudfront:invalidate-staging": "aws cloudfront --profile papagere create-invalidation --distribution-id E2ZR8K942YIJVW --paths \"/*\"",
    "aws:cloudfront:invalidate-prod": "aws cloudfront --profile papagere create-invalidation --distribution-id E16JX0X01NACTY --paths \"/*\""
  },
